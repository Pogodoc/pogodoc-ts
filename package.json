    "@types/validator": "^13.15.1",
    "@vitest/coverage-v8": "^3.2.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "validator": "^13.15.15",
    "vitest": "^3.2.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "js-base64": "^3.7.7",
    "node-fetch": "^3.3.2",
