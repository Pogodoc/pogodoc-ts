{
  "name": "@pogodoc/sdk",
  "version": "0.8.4",
  "description": "Pogodoc SDK for TypeScript",
  "keywords": [
    "pogodoc",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/Pogodoc/pogodoc-ts"
  },
  "type": "commonjs",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "function-test": "tsx src/test.ts",
    "build": "rimraf dist && tsc --project tsconfig.json && copyfiles package.json dist",
